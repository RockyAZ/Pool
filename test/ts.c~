#include <stdio.h>
int *res()
{
	int *i[2] = { 3, 2 };
	return (*i);
}

void loop()
{
	int i;

	i = 2;
	while (i != 1)
		i = 2;
}

int main()
{
    write(1, "c", 1);
    printf("KURVA1");
	write(1, "b", 1);
	printf("KURVA2");

	loop();
	return (0);
}
